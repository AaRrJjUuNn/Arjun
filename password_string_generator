import random


def random_number():
    X = random.randint(0, 10)

    return X


c = random.randint(0, 10)
b = random.randint(11, 21)  # if both randomly choose 10 code would not run
while c < b:
    lc = []
    chars = 'QWERTYUIOPASDFGHJKLZXCVBNM1234567890qwertyuiopasdfghjklzxcvbnm !@#$%^&*()_+{}[]:,./\|'

    for i in range(random_number()):
        c += 1
        # print(random.choice(chars)), this prints the characters, but has no meaning because it is not stored anywhere
        ppp = random.choice(chars)  # normally end is \n

        print(ppp, end='')   # normally end is \n
        # end=""
# investigate randomness
